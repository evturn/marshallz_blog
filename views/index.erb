

<div class="container">
	<ul>
		<% @entries.each do |entry| %>
			<li>
				<ul>
					<div class="entry">
					<h3>
						<a href="/entries/<%= entry.id %>"><%= entry.title.upcase %></a>
					</h3>
						<li>
							<%= entry.body %>
						</li>
					</div>
				</ul>	
			</li>
		<% end %>
	</ul>
</div>